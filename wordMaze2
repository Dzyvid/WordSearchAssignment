package wordMaze;
import java.io.*;
import java.util.Arrays;

public class wordMaze2 {
	public static void getMaze() 
	{
		try {
		BufferedReader in = new BufferedReader(new FileReader("wordMaze.txt")); //change textfile name!!!!
		String[] readinArray;
		String readin = in.readLine();
		if (readin == null) {
			in.close();
		}
		readinArray = readin.split(" ");
		int size = readinArray.length;
		char[][] grid = new char [size][size];
		for(int i = 0; i < grid.length; i++) {
			for(int j = 0; j < grid[i].length; j++) {
				grid[i][j] = readinArray[j].charAt(0);
				System.out.print(grid[i][j] + " "); //Don't need l8r
			}
			System.out.println(""); //Don't need l8r
			try {
			readinArray = in.readLine().split(" ");
			} 
			catch(NullPointerException e) {
				
			}
		}
		}
		catch (IOException e) {
			System.out.println("File Not Found");
		}
	}

	public static void main(String[] args)
	{
		getMaze();
	}
}
